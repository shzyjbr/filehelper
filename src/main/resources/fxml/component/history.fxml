<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../../css/history.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zzk.filehelper.controller.component.HistoryController">
   <children>
      <AnchorPane fx:id="topBar" maxHeight="60.0" prefHeight="60.0" styleClass="topBar" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <StackPane fx:id="backStackPane" maxHeight="40.0" maxWidth="40.0" onMouseReleased="#backToMain" prefHeight="150.0" prefWidth="200.0" styleClass="backStackPane" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="10.0">
               <children>
                  <Region maxHeight="20.0" maxWidth="20.0" prefHeight="20.0" prefWidth="20.0" />
               </children>
            </StackPane>
            <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="40.0" prefHeight="40.0" text="历史" textAlignment="CENTER" AnchorPane.leftAnchor="70.0" AnchorPane.topAnchor="10.0">
               <font>
                  <Font name="Microsoft YaHei" size="22.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="60.0">
         <children>
            <ListView prefHeight="200.0" prefWidth="200.0" styleClass="historyListView" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
